{
  "name": "dangserver",
  "version": "0.0.0",
  "packageManager": "pnpm@8.1.0",
  "description": "",
  "license": "MIT",
  "homepage": "https://github.com/cwanii66/dangserver#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cwanii66/dangserver.git"
  },
  "bugs": "https://github.com/cwanii66/dangserver/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=dev nodemon --watch src/ -e ts --exec ts-node ./src/app.ts",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.1",
    "@types/koa": "^2.13.6",
    "@types/koa-json": "^2.0.20",
    "@types/koa-router": "^7.4.4",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.1",
    "jsonwebtoken": "^9.0.0",
    "koa": "^2.14.1",
    "koa-body": "^6.0.1",
    "koa-json": "^2.0.2",
    "koa-router": "^12.0.0",
    "log4js": "^6.9.1",
    "nodemon": "^2.0.22"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.2",
    "@antfu/ni": "^0.21.2",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.15.11",
    "bumpp": "^9.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0",
    "esno": "^0.16.3",
    "lint-staged": "^13.2.0",
    "pnpm": "^8.1.0",
    "rimraf": "^4.4.1",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vitest": "^0.29.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
